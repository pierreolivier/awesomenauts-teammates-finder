{
	"manifest_version": 2,
	"name": "Steam Teammates Finder",
	"version": "0.21",
	"minimum_chrome_version": "23",
	"icons": {
		"16": "icon_16.png",
		"128": "icon_128.png"
	},
	"app": {
		"background": {		
			"scripts": ["main.js"]
		}
	},
	"permissions": [
		"webview",
		"storage",
		"http://steamcommunity.com/",
		"https://steamcommunity.com/",
		"https://raw.githubusercontent.com/"
	]
}
